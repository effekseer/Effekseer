namespace Effekseer.FB;

struct Vec3F {
  x:float;
  y:float;
  z:float;
}

struct RefMinMax {
  min:int;
  max:int;
}

struct IntRef {
  ref_eq:int;
  value:int;
}

struct FloatRef {
  ref_eq:int;
  value:float;
}

struct IntRange {
  ref_eq:RefMinMax;
  min:int;
  max:int;
}

struct FloatRange {
  ref_eq:RefMinMax;
  min:float;
  max:float;
}

struct Vec3FRange {
  ref_eq:RefMinMax;
  min:Vec3F;
  max:Vec3F;
}
